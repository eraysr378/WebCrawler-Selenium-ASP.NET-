@model IEnumerable<WebApp1.Models.SearchedItem>


<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.ItemId)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.XPath)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Text)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.EventType)
			</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.ItemId)
			    </td>
				<td>
					@Html.DisplayFor(modelItem => item.XPath)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Text)
				</td>

				<td>
					@Html.DisplayFor(modelItem => item.EventType)
				</td>
				<td>
					<a id="Delete" href="/Home/DeleteRead/@item.ItemId" onclick="return confirm('Are you sure?')" class="btn btn-danger ">Delete</a>
				</td>
			</tr>
		}
	</tbody>
</table>



